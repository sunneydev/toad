{
  "name": "@toad/cli",
  "version": "0.0.1",
  "description": "A CLI tool for Toad",
  "publishConfig": {
    "access": "public"
  },
  "bin": {
    "toad": "./dist/index.mjs"
  },
  "type": "module",
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "exports": {
    "require": "./dist/index.js",
    "import": "./dist/index.mjs"
  },
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "prepublishOnly": "pnpm run build"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^18.16.3",
    "tsup": "6.7.0",
    "tsconfig": "workspace:*",
    "typescript": "5.0.4"
  },
  "dependencies": {
    "shared": "workspace:*",
    "@sunney/requests": "^0.2.3",
    "commander": "^10.0.1",
    "dotenv": "^16.0.3",
    "file-system-cache": "^2.1.1"
  }
}
