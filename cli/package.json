{
  "name": "@sunney/toad-cli",
  "version": "0.1.0",
  "private": true,
  "exports": "./dist/index.mjs",
  "bin": {
    "toad": "dist/index.mjs"
  },
  "engines": {
    "node": ">=14.16"
  },
  "scripts": {
    "typecheck": "tsc",
    "build": "tsup src/index.ts --format esm --clean --sourcemap --minify --metafile",
    "prestart": "npm run build",
    "dev": "tsup src/index.ts --format esm --watch --clean --onSuccess \"node dist/index.mjs\"",
    "start": "node dist/index.mjs",
    "lint": "eslint src/ --fix",
    "lint:check": "eslint src/ --cache --cache-strategy content"
  },
  "dependencies": {
    "inquirer": "^9.1.1",
    "yaml": "^2.1.1"
  },
  "devDependencies": {
    "@types/node": "18.0.0",
    "@types/yaml": "^1.9.7",
    "eslint": "8.18.0",
    "tsup": "^6.2.1",
    "typescript": "4.7.4"
  }
}
